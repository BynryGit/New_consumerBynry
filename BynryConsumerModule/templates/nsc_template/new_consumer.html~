<script>
    function clear_filter(){
        $( "#filter_div" ).load(" #filter_div" );
        setTimeout(function () {
            $('.select2').select2();
            //initTable1();
            src = "{% static 'assets/pages/scripts/components-date-time-pickers.min.js' %}";
            $('script[src="' + src + '"]').remove();
            $('<script>').attr('src', src).appendTo('head');
        }, 1000);
    }
</script>

