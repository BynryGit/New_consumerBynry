    <script>

    $("#verify_city").select2();

    function registered_consumer() {
        $("#div11").css('display', 'none');
        $("#div5").css('display', 'none');
        $("#div4").css('display', 'none');
        $("#div6").css('display', 'none');
        $("#div1").css('display', 'block');
    }

    function new_consumer() {
        $("#div11").css('display', 'none');
        $("#div5").css('display', 'none');
        $("#div4").css('display', 'none');
        $("#div6").css('display', 'block');
        $("#div1").css('display', 'none');
    }

    function unregistered_consumer() {
        $("#div1").css('display', 'none');
        $("#div5").css('display', 'none');
        $("#div4").css('display', 'block');
        $("#div6").css('display', 'block');
    }

    function save_new_consumer() {
        if (validate_new_consumer_data()) {
            var formData = new FormData();
            formData.append("city", $("#new_city").val());
            formData.append("name", $("#new_cname").val());
            formData.append("email", $("#new_cemail_id").val());
            formData.append("contact_no", $("#new_cmobile_no").val());

            $.ajax({
                type: "POST",
                url: '/cti-crm/save-new-consumer/',
                data: formData,
                cache: false,
                processData: false,
                contentType: false,

                success: function (response) {
                    if (response.success == 'true') {

                        $("#new_city").attr("readonly", "readonly");
                        $("#new_cname").attr("readonly", "readonly");
                        $("#new_cemail_id").attr("readonly", "readonly");
                        $("#new_cmobile_no").attr("readonly", "readonly");
                        $("#save_btn").attr("hidden", "hidden");
                        $("#div4").css('display', 'block');
                        
                    }
                    if (response.success == "false") {
                        //$("#div5").css('display','block');
                    }
                },
                beforeSend: function () {
                    $("#processing").css('display', 'block');
                },
                complete: function () {
                    $("#processing").css('display', 'none');
                },
                error: function (response) {
                    alert("_Error");
                }
            });
        }
    }

    function validate_new_consumer_data() {
        if (CheckNewCity("#new_city") & CheckConsumerName("#new_cname") & CheckConsumerContactNo("#new_cmobile_no") & CheckConsumerEmail("#new_cemail_id")) {
            return true
        }
        return false
    }
    function CheckNewCity(new_city) {
        if ($(new_city).val() != ' ' && $(new_city).val() != null) {
            $(new_city_error).css("display", "none");
            return true;
        } else {
            $(new_city_error).css("display", "block");
            $(new_city_error).text("Please select City");
            return false;
        }
    }
    function CheckConsumerName(new_cname) {
        consumer_name = $(new_cname).val()
        var Pattern = /[A-Za-z]+/;

        if (Pattern.test(consumer_name)) {
            $(new_cname_error).css("display", "none");
            return true;
        }
        else {
            $(new_cname_error).css("display", "block");
            $(new_cname_error).text("Please enter valid Consumer Name");
            return false;
        }
    }
    function CheckConsumerContactNo(new_cmobile_no) {
        mobile = $(new_cmobile_no).val()
        var phoneNumberPattern = /^[789]\d{9}$/;
        if (phoneNumberPattern.test(mobile)) {
            $(new_cmobile_no_error).css("display", "none");
            return true;
        } else {
            $(new_cmobile_no_error).css("display", "block");
            $(new_cmobile_no_error).text("Please enter valid Mobile No");
            return false;
        }
    }

    function CheckConsumerEmail(new_cemail_id) {
        Email = $(new_cemail_id).val()

        var namePattern = /[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,3}$/;
        if (Email == '') {
            $(new_cemail_error).css("display", "none");
            return true;
        }
        else if (namePattern.test(Email)) {
            $(new_cemail_error).css("display", "none");
            return true;
        }
        else {
            $(new_cemail_error).css("display", "block");
            $(new_cemail_error).text("Please enter valid email");
            return false;
        }
    }

    function verify_consumer() {
        if (validate_verify()) {
            var formData = new FormData();
            formData.append("city_id", $("#verify_city").val());
            formData.append("consumer_no", $("#verify_consumer_no").val());

            $.ajax({
                type: "POST",
                url: '/cti-crm/verify-new-consumer/',
                data: formData,
                //cache: false,
                processData: false,
                contentType: false,

                success: function (response) {
                    if (response.success == 'true') {
                        $("#c_no").val(response.consumer_no);
                        $("#c_name").val(response.consumer_name);
                        $("#c_mobile_no").val(response.mobile_number);
                        $("#c_email").val(response.email_id);

                        $("#div1").css('display', 'none');
                        $("#div5").css('display', 'none');
                        $("#div2").css('display', 'block');
                        $("#div3").css('display', 'block');
                        $("#div7").css('display', 'block');
                        $("#div8").css('display', 'block');
                        $("#div9").css('display', 'block');
                        $("#div10").css('display', 'block');
                    }
                    if (response.success == "false") {
                        $(verify_consumer_no_error).css("display", "block");
                        $(verify_consumer_no_error).text("Consumer Number does not exist");
                        //$("#div5").css('display', 'block');
                        //$("#div4").css('display','block');
                    }
                },
                beforeSend: function () {
                    $("#processing").css('display', 'block');
                },
                complete: function () {
                    $("#processing").css('display', 'none');
                },
                error: function (response) {
                    alert("_Error");
                }
            });
        }
    }

    function validate_verify() {
        if (CheckCity("#verify_city") & CheckConsumerNo("#verify_consumer_no")) {
            return true
        }
        return false
    }
    function CheckCity(city) {
        if ($(city).val() != ' ' && $(city).val() != null) {
            $(verify_city).parent().children('.error').css("display", "none");
            return true;
        } else {
            $(verify_city).parent().children('.error').css("display", "block");
            $(verify_city).parent().children('.error').text("Please select City");
            return false;
        }
    }
    function CheckConsumerNo(consumer_no) {
        consumer_no = $(consumer_no).val()
        var Pattern = /^[a-zA-Z0-9!@#$%^&*]{12}$/;

        if (Pattern.test(consumer_no)) {
            $(verify_consumer_no_error).css("display", "none");
            return true;
        }
        else {
            $(verify_consumer_no_error).css("display", "block");
            $(verify_consumer_no_error).text("Consumer Number does not exist");
            return false;
        }
    }


  





    </script>
</body>

</html>